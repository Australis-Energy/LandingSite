# Australis Energy - Environment Variables Example
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Azure Configuration
VITE_AZURE_MAPS_KEY=your_azure_maps_key_here
VITE_AZURE_STORAGE_ACCOUNT=your_storage_account_here
VITE_AZURE_STORAGE_KEY=your_storage_key_here

# API Configuration
VITE_API_BASE_URL=https://australis-energy-dev-app.azurewebsites.net/api
VITE_API_KEY=your_api_key_here

# Azure Functions
VITE_GEOSERVICES_FUNCTION_URL=your_geoservices_function_url_here
VITE_GEOSERVICES_FUNCTION_KEY=your_function_key_here

# Communications Function (SendEmailFunction)
VITE_COMMUNICATIONS_FUNCTION_URL=https://your-communications-function-app.azurewebsites.net
VITE_COMMUNICATIONS_FUNCTION_KEY=your_communications_function_key_here

# Azure Service Bus (if needed for client-side operations)
VITE_SERVICE_BUS_CONNECTION_STRING=your_service_bus_connection_string_here

# Application Insights (if using client-side telemetry)
VITE_APPINSIGHTS_INSTRUMENTATION_KEY=your_appinsights_key_here

# Environment
VITE_APP_ENV=development
